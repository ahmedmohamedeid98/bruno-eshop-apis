meta {
  name: statistics
  type: http
  seq: 3
}

get {
  url: {{URL}}/api/v1/vendor/orders/statistics
  body: none
  auth: inherit
}

headers {
  Accept: application/json
  Authorization: Bearer {{VENDOR_TOKEN}}
}

settings {
  encodeUrl: true
}

example {
  name: success
  
  request: {
    url: {{URL}}/api/v1/vendor/orders/statistics
    method: GET
    mode: none
    headers: {
      Accept: application/json
      Authorization: Bearer {{VENDOR_TOKEN}}
    }
  }
  
  response: {
    headers: {
      Server: nginx/1.25.4
      Content-Type: application/json
      Transfer-Encoding: chunked
      Connection: keep-alive
      Vary: Accept-Encoding
      X-Powered-By: PHP/8.3.8
      Cache-Control: no-cache, private
      Date: Sun, 28 Dec 2025 14:36:17 GMT
      Access-Control-Allow-Origin: *
      Content-Encoding: gzip
    }
  
    status: {
      code: OK
      text: 200
    }
  
    body: {
      type: json
      content: '''
        {
            "success": true,
            "message": "Vendor order statistics retrieved successfully",
            "data": {
                "pending": 0,
                "processing": 0,
                "ready": 0,
                "collected": 1,
                "cancelled": 0,
                "total_earnings": "6.00"
            }
        }
      '''
    }
  }
}
